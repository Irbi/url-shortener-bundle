{
    "type": "symfony-bundle",
    "license": "MIT",
    "name": "irbi/url-shortener",
    "description": "Urls shortener",
    "version": "1.1.0",
    "require": {
        "php": "^7.3.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "hashids/hashids": "^2.0",
        "jms/serializer-bundle": "^3.3",
        "sensio/framework-extra-bundle": "^5.3",
        "symfony/console": "4.3.*",
        "symfony/dotenv": "4.3.*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "4.3.*",
        "symfony/orm-pack": "^1.0",
        "symfony/yaml": "4.3.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "irbi\\UrlShortenerBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "irbi\\UrlShortenerBundle\\Tests\\": "tests"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.3.*"
        }
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.11"
    }
}
